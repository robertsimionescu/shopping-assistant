<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gagauzia\ServicesBundle\Entity\Spider" table="spider">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="partNumber" type="string" column="part_number" length="128" nullable="true"/>
    <field name="title" type="string" column="title" length="255" nullable="true"/>
    <field name="price" type="decimal" column="price" precision="7" scale="2" nullable="true"/>
    <field name="url" type="string" column="url" length="255" nullable="true"/>
    <field name="idEmag" type="integer" column="id_emag" nullable="false"/>
    <field name="idSite" type="integer" column="id_site" nullable="false"/>
  </entity>
</doctrine-mapping>
